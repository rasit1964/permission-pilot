    <string name="permissions_details_description_restrictions_caveat_description">A mas felhasznaloi profilokban telepitett alkalmazasok ki vannak zarva a fenti szamitasbol az Android korlatozasai miatt.</string>
    <string name="permissions_details_empty_filter_message">Egyetlen alkalmazas sem felel meg a jelenlegi szuroknek. Koppints a szuro ikonra a modositashoz.</string>
    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Lehetove teszi az alkalmazasok szamara a Near Field Communication (NFC) cimkekkel, kartyakkal es olvasokkal valo kommunikaciot.</string>
    <string name="permission_manage_media_label">Media kezelese</string>
    <string name="permission_manage_media_description">Lehetove teszi az alkalmazasok szamara a mas alkalmazasokkal letrehozott mediafajlok modositasat vagy torleser ANELKUL, hogy kerdeznenenek. Az alkalmazasoknak rendelkezniuk kell a fajlok es media hozzaferesi engedelyevel.</string>
    <string name="permission_usage_data_access_label">Hasznalati adatok hozzaferese</string>
    <string name="permission_usage_data_access_description">Lehetove teszi az alkalmazasok szamara, hogy kovetik, milyen mas alkalmazasokat hasznalsz es milyen gyakran, azonositjak a szolgaltatodat, nyelvi beallitasokat es egyeb hasznalati adatokat.</string>
    <string name="permission_modify_system_settings_label">Rendszerbeallitasok modositasa</string>
    <string name="permission_modify_system_settings_description">Lehetove teszi az alkalmazasok szamara a rendszerbeallitasi adatok modositasat. A rosszindulatu alkalmazasok megrongalhatjak a rendszer konfiguraciojadat.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth hirdetes</string>
    <string name="permission_bluetooth_advertise_description">Lehetove teszi az alkalmazasok szamara, hogy hirdetesenek a kozeli Bluetooth eszkozoknek.</string>
    <string name="permission_get_accounts_label">Fiokok lekerese</string>
    <string name="permission_get_accounts_description">Lehetove teszi az alkalmazasok szamara a telefon altal ismert fiokok listajanak lekeresest. Ez magaban foglalhatja az altalad telepitett alkalmazasok altal letrehozott fiokokat is.</string>
    <string name="permission_access_background_location_label">Hatter-helymeghatarozi hozzaferes</string>
    <string name="permission_access_background_location_description">Lehetove teszi az alkalmazasok szamara a hely elerest barmikor, meg akkor is, ha nincsenek hasznalva.</string>
    <string name="permission_access_media_location_label">Media hely hozzaferes</string>
    <string name="permission_access_media_location_description">Lehetove teszi az alkalmazasok szamara a helyadatok olvasasat a mediagyujtemenyedbol.</string>
    <string name="permission_manage_accounts_label">Fiokok kezelese</string>
    <string name="permission_manage_accounts_description">Lehetove teszi az alkalmazasok szamara az eszkozon levo fiokok kezeleset.</string>
    <string name="permission_wifi_control_label">WiFi vezerles</string>
    <string name="permission_wifi_control_description">Lehetove teszi az alkalmazasok szamara a WiFi be- vagy kikapcsolasat, WiFi halozatok kereseset es csatlakozast, halozatok hozzaadasat vagy eltavolitasat, vagy helyi hotspot inditasat.</string>
    <string name="permission_change_network_state_label">Halozat allapotanak modositasa</string>
    <string name="permission_change_network_state_description">Lehetove teszi az alkalmazasok szamara a halozati kapcsolat allapotanak megvaltoztatasat.</string>
    <string name="permission_read_sync_settings_label">Szinkronizalasi beallitasok olvasasa</string>
    <string name="permission_read_sync_settings_description">Lehetove teszi az alkalmazasok szamara egy fiok szinkronizalasi beallitasainak olvasasat.</string>
    <string name="permission_install_unknown_apps_label">Ismeretlen alkalmazasok telepitese (ismeretlen forrasok)</string>
    <string name="permission_install_unknown_apps_description">Lehetove teszi az alkalmazasok szamara mas alkalmazasok telepiteset (oldalrol betoltes) az eszkozodre. Ez veszelyeztetheti az eszkozt es az adataidat.</string>
    <string name="permission_all_files_access_label">Osszes fajl hozzaferes</string>
    <string name="permission_all_files_access_description">Lehetove teszi az alkalmazasok szamara az eszkozodon es barmely csatlakoztatott taroloeszkozon levo osszes fajl olvasasat, modositasat es torleser. Az ezzel az engedellyel rendelkezo alkalmazasok az alkalmazashoz nem kapcsolodo fajlokhoz is hozzaferhetnek, ertesites NELKUL.</string>
    <string name="permission_appear_on_top_label">Megjelenes felul</string>
    <string name="permission_appear_on_top_description">Lehetove teszi az alkalmazasok szamara, hogy dolgokat jelenitsenek meg a hasznalt alkalmazasok felett (beleertve a reklamokat). Ez zavarhatja mas alkalmazasok hasznalatat.</string>
    <string name="permission_files_and_media_label">Fajlok es media</string>
    <string name="permission_files_and_media_description">Lehetove teszi az alkalmazasok szamara a fajlokhoz es mediahoz valo hozzaferest az eszkozodon.</string>
    <string name="permission_access_to_media_only_label">Csak media hozzaferes</string>
    <string name="permission_access_to_media_only_description">Lehetove teszi az alkalmazasok szamara csak a mediahoz valo hozzaferest az eszkozodon.</string>
    <string name="permission_access_precise_location_label">Pontos/reszletes hely hozzaferes</string>
    <string name="permission_access_precise_location_description">Lehetove teszi az alkalmazasok szamara az eszkozod pontos/reszletes helyenek elerest. Ez a legtobb alkalmazasnal NEM ajanlott (kiveve nehany alkalmazast, mint a terkepek, taxis szolgaltatasok, eszkozkoveto szolgaltatasok, mint a Find My Device stb.).</string>
    <string name="permission_access_approximate_location_label">Kozelito hely hozzaferes</string>
    <string name="permission_access_approximate_location_description">Lehetove teszi az alkalmazasok szamara az eszkozod kozelito helyenek elerest. Ez az engedely elegseges a legtobb alkalmazas szamara, amelyeknek szuksege van a helyadataidra az alapfunkciokhoz.</string>
    <string name="permission_ignore_battery_optimizations_label">Akkumulator-optimalizalas figyelmen kivul hagyasa</string>
    <string name="permission_ignore_battery_optimizations_description">Lehetove teszi az alkalmazasok szamara, hogy akadalytalanul fussanak a hatterben. Ez lemeritiheti az akkumulatort. Csak olyan alkalmazasokhoz ajanlott, amelyek folyamatos/gyakori biztonsagi menteseket es/vagy szinkronizalasokat vegeznek (pl. Google Photos), nagyon gyakran hasznalt alkalmazasok (pl. e-mail, chat, SMS stb.), valamint azok az alkalmazasok, amelyeknek allo ertesitesuk van.</string>
    <string name="permission_notification_access_label">Ertesitesek hozzaferese</string>
    <string name="permission_notification_access_description">Lehetove teszi az alkalmazasok szamara az osszes ertesitesed olvasasat, beleertve a szemelyes adatokat (pl. nevjegyek nevei es a kapott uzenetek szovege), az ertesitesek halasztasat vagy elveteset, vagy a bennuk levo muveleti gombok aktivalasat (pl. telefonhivasok fogadasa, SMS/e-mail torlese stb.), valamint a Ne zavarjanak (DND) be- vagy kikapcsolasat es a kapcsolodo beallitasok modositasat.</string>
    <string name="permission_do_not_disturb_permission_label">Ne zavarjanak (DND) engedely</string>
    <string name="permission_do_not_disturb_permission_description">Lehetove teszi az alkalmazasok szamara a Ne zavarjanak (DND) be- vagy kikapcsolasat es a kapcsolodo beallitasok modositasat.</string>
    <string name="permission_alarms_and_reminders_label">Risztok es emlekeztetok</string>
    <string name="permission_alarms_and_reminders_description">Lehetove teszi az alkalmazasok szamara risztok beallitasat es idoerzekeny muveletek utemezeset. Ez lehetove teszi az alkalmazasoknak, hogy a hatterben fussanak, ami tobb akkumulatort haszmalhat. Ennek az engedelynek a kikapcsolasa egy alkalmazasnal az adott alkalmazas altal utemezett meglevo risztok es idoalapu esemenyek mukodesenek leallasahoz vezet.</string>
    <string name="permission_turn_screen_on_label">Kepernyo bekapcsolasa</string>
    <string name="permission_turn_screen_on_description">Lehetove teszi az alkalmazasok szamara az eszkoz kepernyojenek bekapcsolasat, amikor szukseguk van a figyelmedre, peldaul risztok vagy bejovo hivasok eseten.</string>
    <string name="permission_full_screen_intent_label">Teljes kepernyos ertesitesek</string>
    <string name="permission_full_screen_intent_description">Lehetove teszi az alkalmazasok szamara teljes kepernyos ertesitesek megjeleniteset, amelyek atvaltjak a kepernyot, amikor az eszkoz zarolva van vagy hasznalva. Altalaban risztokhaz, hivasokhoz es mas idoerzekeny esemenyekhez hasznaljak.</string>
    <string name="permission_interact_across_profiles_label">Osszekotott munkahelyi es szemelyes alkalmazasok</string>
    <string name="permission_interact_across_profiles_description">Lehetove teszi az alkalmazasok szamara a munkahelyi es szemelyes profilok kozotti kommunikaciot, adatok es funkciok megosztasat a profilhatarokon at.</string>
    <string name="permission_loader_usage_stats_label">Betolto hasznalati statisztikak</string>
    <string name="permission_loader_usage_stats_description">Lehetove teszi az alkalmazasok szamara statisztikak gyujteset az adatbetolto hasznalarol az eszkozon.</string>
    <string name="permission_premium_sms_services_label">Premium SMS hozzaferes</string>
    <string name="permission_premium_sms_services_description">Lehetove teszi az alkalmazasok szamara premium szoveges uzenetszolgaltatasok hasznalatat. Ez penzbe kerul.</string>
    <string name="permission_unlimited_data_access_label">Korlatlan adat-hozzaferes</string>
    <string name="permission_unlimited_data_access_description">Lehetove teszi az alkalmazasok szamara a korlatlan adatokhoz valo hozzaferest. Az alkalmazasok adatokat fogyaszthatnak meg akkor is, ha az Adattakarekos mod be van kapcsolva.</string>
    <string name="permission_group_calendar_label">Naptar</string>
    <string name="permission_group_calendar_description"><![CDATA[Naptarral kapcsolatos engedelyek, pl. bejegyzesek megtekintese es szerkesztese.]]></string>
    <string name="permission_group_files_label">Fajlok es media</string>
    <string name="permission_group_files_description">Fajlokkal kapcsolatos engedelyek. Hozzaferes a tarolohoz, dokumentumokhoz es mediahoz.</string>
    <string name="permission_group_other_label">Egyeb</string>
    <string name="permission_group_other_description">Egyeb engedelyek meghatarozott kategoria nelkul.</string>
