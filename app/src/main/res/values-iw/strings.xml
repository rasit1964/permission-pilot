    <string name="permission_group_messaging_label">הודעות</string>
    <string name="permission_group_messaging_description">SMS, MMS ועוד. כל מה שקשור להודעות.</string>
    <string name="permission_group_connectivity_label">קישוריות</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth ועוד. הרשאות הקשורות לתקשורת עם העולם החיצון.</string>
    <string name="permission_group_sensors_label">חיישנים</string>
